<h1>Articles</h1>

<style>
  body {
    background-color: #121212;
    color: #ffffff;
  }

  ul {
    list-style-type: none;
  }

  li {
    background-color: #333333;
    margin: 5px 0;
    padding: 10px;
    border-radius: 5px;
  }
</style>

<ul>
  <% @articles.each do |article| %>
    <li>
      <%= article.title %>
      <p><%= article.body %></p>
      <p><%= article.created_at %></p>
      <p><%= article.updated_at %></p>
    </li>
  <% end %>
</ul>

